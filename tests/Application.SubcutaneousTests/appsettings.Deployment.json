{
  "AllowedHosts": "*",
  "AmazonS3Settings": {
    "ServiceUrl": "http://localhost:9000",
    "AccessKey": "$S3_KEY",
    "SecretKey": "$S3_SECRET",
    "BucketName": "the-template-project",
    "PreSignedUrlExpirationInMinutes": 1440,
    "Protocol": 1
  },
  "SecuritySettings": {
    "JwtSettings": {
      "SecretKey": "$JWT_SECRET",
      "ExpireTimeAccessTokenInMinute": 0.17,
      "ExpireTimeRefreshTokenInDay": 0.0001736
    }
  },
  "ForgotPasswordSettings": {
    "Uri": "http://localhost:3000/reset-password",
    "ExpiredTimeInHour": 0.004722222
  },
  "DatabaseSettings": {
    "DatabaseConnection": "$DB_CONNECTION"
  },
  "MemoryCacheSettings": {
    "DefaultCacheExpirationInMinute": 5
  },
  "LocalizationSettings": {
    "DefaultCulture": "vi",
    "SupportedCultures": ["en", "vi"],
    "TranslationCacheInMinutes": 30
  },
  "IdentityCacheSettings": {
    "UserRoles": {
      "ExpirationMinutes": 30
    },
    "RolePermissions": {
      "ExpirationMinutes": 60
    },
    "UserPermissions": {
      "ExpirationMinutes": 60
    }
  }
}
