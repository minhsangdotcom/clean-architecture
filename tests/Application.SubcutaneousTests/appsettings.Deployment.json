{
  "AllowedHosts": "*",
  "AmazonS3Settings": {
    "ServiceUrl": "http://localhost:9000",
    "AccessKey": "$S3_KEY",
    "SecretKey": "$S3_SECRET",
    "BucketName": "$S3_BUCKET",
    "PreSignedUrlExpirationInMinutes": 1440,
    "Protocol": 1
  },
  "SecuritySettings": {
    "Default": {
      "JwtSettings": {
        "Name": "Default",
        "SecretKey": "$JWT_SECRET",
        "AccessTokenExpirationInMinutes": 15,
        "RefreshTokenExpirationInDays": 7
      }
    }
  },
  "ForgotPasswordSettings": {
    "Uri": "http://localhost:3000/reset-password",
    "ExpiredTimeInHour": 1,
    "TemplateName": "ForgotPassword.html"
  },
  "DatabaseSettings": {
    "DatabaseConnection": "$DB_CONNECTION"
  },
  "MemoryCacheSettings": {
    "DefaultExpirationInMinutes": 5
  },
  "LocalizationSettings": {
    "DefaultCulture": "vi",
    "SupportedCultures": ["en", "vi"],
    "TranslationCacheDurationInMinutes": 30
  },
  "IdentityCacheSettings": {
    "UserRoles": {
      "ExpirationInMinutes": 30
    },
    "RolePermissions": {
      "ExpirationInMinutes": 60
    },
    "UserPermissions": {
      "ExpirationInMinutes": 60
    }
  }
}
